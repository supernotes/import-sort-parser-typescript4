{
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "bracketSpacing": true,
    "semi": true,
    "proseWrap": "preserve",
    "arrowParens": "always",
    "plugins": ["prettier-plugin-organize-imports"],
    "overrides": [
      {
        "files": ["*.html"],
        "options": {
          "parser": "html"
        }
      },
      {
        "files": ["*.ts", "*.tsx"],
        "options": {
          "parser": "typescript"
        }
      },
      {
        "files": ["*.json", "*.code-workspace", ".prettierrc", ".eslintrc", ".swcrc"],
        "options": {
          "parser": "json"
        }
      }
    ]
  }
  